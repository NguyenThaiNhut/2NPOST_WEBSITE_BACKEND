<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Get CRUD</title>
</head>

<style>
    table {
        font-family: arial, sans-serif;
        border-collapse: collapse;
        width: 100%;
    }

    td,
    th {
        border: 1px solid #dddddd;
        text-align: left;
        padding: 8px;
    }

    tr:nth-child(even) {
        background-color: #dddddd;
    }
</style>

<body>
    <table>
        <thead>
            Get Users CRUD
        </thead>
        <tbody>
            <tr>
                <th>ID</th>
                <th>Firstname</th>
                <th>Lastname</th>
                <th>Address</th>
                <th></th>
            </tr>
            <% for (var i=0; i < dataUser.length; /* I save the data in a variable 'quotation' , I don't know how you
                named your variable */ i++) { %>
                <tr>
                    <td>
                        <%= dataUser[i].id %>
                    </td>
                    <td>
                        <%= dataUser[i].firstName %>
                    </td>
                    <td>
                        <%= dataUser[i].lastName %>
                    </td>
                    <td>
                        <%= dataUser[i].address %>
                    </td>
                    <td>
                        <a href="/edit-crud?id=<%= dataUser[i].id %>">Edit</a>
                        <a href="/delete-crud?id=<%= dataUser[i].id %>">Delete</a>
                    </td>
                </tr>
                <% } %>
        </tbody>
    </table>
</body>

</html>